{
  "name": "@edenomics/ui",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    "./card": "./src/card.tsx"
  },
