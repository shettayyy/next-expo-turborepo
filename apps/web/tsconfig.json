{
  "extends": "@shettayyy-tools/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@web/libs/*": ["./libs/*"]
    },
    "types": ["@serwist/next/typings"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules", "public/sw.js"]
}
