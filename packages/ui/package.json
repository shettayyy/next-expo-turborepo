  },
  "peerDependencies": {
    "react": "^19"
  },
  "devDependencies": {
    "@edenomics-tools/eslint-config": "workspace:*",
    "@edenomics-tools/tailwind-config": "workspace:*",
    "@edenomics-tools/typescript-config": "workspace:*",
    "@types/react": "^19.0.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.1",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3"
  }
}
